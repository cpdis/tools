{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Basic React boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cpdis/tools/tree/master/react-boilerplate"
  },
  "keywords": [
    "Node",
    "React"
  ],
  "author": "Colin Dismuke",
  "license": "Apache-2.0",
  "dependencies": {
    "express": "^4.16.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.1.0"
  }
}
